<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Kashif Mubarak</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/font-awesome.css" rel="stylesheet" />
</head>
<body>
    <div class="container body-content">
        @RenderBody()
        <!-- Footer start -->
        <footer id="footer">
            <div class="container">
                <div class="row">
                    <div class="col-sm-12">
                        <ul class="social-links">
                            <li><a href="https://www.linkedin.com/in/kashifmubarak" class="wow fadeInUp" data-wow-delay=".1s"><i class="fa fa-linkedin"></i></a></li>
                        </ul>
                        <p class="heart">
                            Made with <span class="fa fa-heart fa-2x animated pulse"></span>
                        </p>
                        <p class="copyright">
                            &copy; @DateTime.Now.Year - Kashif Mubarak
                        </p>
                    </div>
                </div><!-- .row -->
            </div><!-- .container -->
        </footer>
        <!-- Footer end -->
        <!-- Scroll to top -->
        <div class="scroll-up">
            <a href="#home"><i class="fa fa-angle-up"></i></a>
        </div>
        <!-- Scroll to top end-->
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
